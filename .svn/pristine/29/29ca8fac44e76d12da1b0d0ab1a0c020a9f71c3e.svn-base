public class LoanSummary implements Bean {

	private String loanID;// ID
	private String picture;// 图片的URL
	private String title;// 主题
	private double amount;// 金额
	private double speedOfProgress;// 进度
	private double rate;// 利率
	private int loanTerm;// 期限 天
	private int measureUnit;

	public LoanSummary() {
		super();
		// TODO Auto-generated constructor stub
	}

	public LoanSummary(String loanID, String picture, String title, double amount, double speedOfProgress, double rate, int loanTerm, int measureUnit) {
		super();
		this.loanID = loanID;
		this.picture = picture;
		this.title = title;
		this.amount = amount;
		this.speedOfProgress = speedOfProgress;
		this.rate = rate;
		this.loanTerm = loanTerm;
		this.measureUnit = measureUnit;
	}

	public String getPicture() {
		return picture;
	}

	public void setPicture(String picture) {
		this.picture = picture;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public double getAmount() {
		return amount;
	}

	public void setAmount(double amount) {
		this.amount = amount;
	}

	public double getSpeedOfProgress() {
		return speedOfProgress;
	}

	public void setSpeedOfProgress(double speedOfProgress) {
		this.speedOfProgress = speedOfProgress;
	}

	public double getRate() {
		return rate;
	}

	public void setRate(double rate) {
		this.rate = rate;
	}

	public int getLoanTerm() {
		return loanTerm;
	}

	public void setLoanTerm(int loanTerm) {
		this.loanTerm = loanTerm;
	}

	public int getMeasureUnit() {
		return measureUnit;
	}

	public void setMeasureUnit(int measureUnit) {
		this.measureUnit = measureUnit;
	}

	public String getLoanID() {
		return loanID;
	}

	public void setLoanID(String loanID) {
		this.loanID = loanID;
	}

}
